extends _layout.jade
block title
  title googlemaps-scrollprevent.js - DOM Ready Example
block content
  h1 DOM Ready Example
  script
    :coffee-script
      $ ->
        $("#btn-start").click ->
          $("iframe[src*='google.com/maps']")
            .scrollprevent {
              printLog:true
            }
            .start()
                    
          $("#btn-stop").click ->
            $("iframe[src*='google.com/maps']").scrollprevent().stop()
        
        $("#btn-start").trigger "click"
