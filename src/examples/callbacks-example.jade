extends _layout.jade
block title
  title #{baseTitle} Callbacks Example
block content
  script
    :coffee-script
      $ ->
        $("#btn-start").click ->
          $("iframe[src*='google.com/maps']")
            .scrollprevent {
              printLog:true
              onMapLock: ->
                alert "Overlay Showed"
              onMapUnlock: ->
                alert "Overlay Hiden"
            }
            .start()

          $("#btn-stop").click ->
            $("iframe[src*='google.com/maps']").scrollprevent().stop()

        $("#btn-start").trigger "click"
