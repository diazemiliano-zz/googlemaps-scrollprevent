extends _layout.jade
block title
  title mapScrollPrevent.js - Start/Stop Example
block content
  h1 Start Stop Example
  script
    :coffee-script
      $ ->
        $("#btn-start").click ->
          $("iframe[src*='google.com/maps']")
            .mapScrollPrevent {
              printLog:true
            }
            .start()

        $("#btn-stop").click ->
          $("iframe[src*='google.com/maps']").mapScrollPrevent().stop()
