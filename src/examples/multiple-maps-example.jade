extends _layout.jade
block title
  title #{baseTitle} Multiple Maps
block localscript
  script(src="../dist/googlemaps-scrollprevent.js")
block content
  h1 Multiple Maps
  script
    :coffee-script
      $ ->
        googleMapSelector = $("iframe[id*='gmap-prevent-']")
        $("#btn-start").click ->
          googleMapSelector
            .scrollprevent {
              printLog:true
            }
            .start()
            
          $("#btn-stop").click ->
            googleMapSelector.scrollprevent().stop()
            
        $("#btn-start").trigger "click"

